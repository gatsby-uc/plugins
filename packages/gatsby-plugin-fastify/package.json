{
  "name": "gatsby-plugin-fastify",
  "description": "Gatsby plugin for integration serving gatsby on Node.js using Fastify.",
  "version": "0.8.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-uc/plugins.git",
    "directory": "packages/gatsby-plugin-fastify"
  },
  "contributors": [
    "Alex Moon <alex.jared.moon@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-fastify",
    "nodejs",
    "server",
    "fastify"
  ],
  "bin": {
    "gserve": "./cli.js"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__,**/*.d.ts\" --extensions \".ts,.js\"",
    "pretest": "cd ../../integration-tests/plugin-fastify/ && yarn run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "fastify-accepts": "^2.2.0",
    "fastify-http-proxy": "^6.2.2",
    "fastify-plugin": "^3.0.1",
    "fastify-static": "^4.6.1",
    "fs-extra": "^10.0.1",
    "gatsby-core-utils": "^3.10.1",
    "gatsby-plugin-utils": "^3.4.2",
    "http-status-codes": "^2.2.0",
    "mime": "^3.0.0",
    "open": "^8.4.0",
    "picomatch": "^2.3.1",
    "pino-pretty": "^7.6.1",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.18.12",
    "babel-jest": "^27.5.1",
    "babel-preset-gatsby-package": "^2.12.1",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1"
  },
  "peerDependencies": {
    "fastify": "^3.19.0",
    "gatsby": "^4.2.0"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
