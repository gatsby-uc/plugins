{
  "name": "gatsby-plugin-fastify",
  "description": "Gatsby plugin for integration serving gatsby on Node.js using Fastify.",
  "version": "0.12.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-uc/plugins.git",
    "directory": "packages/gatsby-plugin-fastify"
  },
  "contributors": [
    "Alex Moon <alex.jared.moon@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-fastify",
    "nodejs",
    "server",
    "fastify"
  ],
  "bin": {
    "gserve": "./dist/cli.js"
  },
  "scripts": {
    "build": "babel src --out-dir ./dist --ignore \"**/__tests__,**/*.d.ts\" --extensions \".ts,.js\"",
    "pretest": "cd ../../integration-tests/plugin-fastify/ && yarn run build",
    "test": "jest --coverage",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "@fastify/accepts": "^4.1.0",
    "@fastify/http-proxy": "^8.4.0",
    "@fastify/middie": "^8.0.0",
    "@fastify/static": "^6.5.1",
    "fastify-plugin": "^4.3.0",
    "fs-extra": "^11.1.0",
    "gatsby-core-utils": "^4.2.0",
    "gatsby-plugin-utils": "^4.2.0",
    "http-status-codes": "^2.2.0",
    "mime": "^3.0.0",
    "open": "^8.4.0",
    "picomatch": "^2.3.1",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@types/connect": "^3.4.35",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.3",
    "@types/mime": "^3.0.1",
    "@types/node": "^18.11.10",
    "@types/picomatch": "^2.3.0",
    "@types/yargs": "^17.0.15",
    "babel-jest": "^29.3.1",
    "babel-preset-gatsby-package": "^3.2.0",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1"
  },
  "peerDependencies": {
    "fastify": "^4.5.3",
    "gatsby": "^4.2.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
