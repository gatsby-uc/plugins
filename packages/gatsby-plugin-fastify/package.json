{
  "name": "gatsby-plugin-fastify",
  "description": "Gatsby plugin for integration serving gatsby on Node.js using Fastify.",
  "version": "0.7.4",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-uc/plugins.git",
    "directory": "packages/gatsby-plugin-fastify"
  },
  "contributors": [
    "Alex Moon <alex.jared.moon@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-fastify",
    "nodejs",
    "server",
    "fastify"
  ],
  "bin": {
    "gserve": "./cli.js"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__,**/*.d.ts\" --extensions \".ts,.js\"",
    "pretest": "cd ../../integration-tests/plugin-fastify/ && yarn run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "fastify-accepts": "^2.1.0",
    "fastify-compress": "^3.7.0",
    "fastify-plugin": "^3.0.1",
    "fastify-static": "^4.5.0",
    "fs-extra": "^10.0.0",
    "open": "^8.4.0",
    "picomatch": "^2.3.1",
    "pino-pretty": "^7.5.1",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.18.10",
    "babel-jest": "^27.4.6",
    "babel-preset-gatsby-package": "^2.6.0",
    "cross-env": "^7.0.3",
    "fastify": "^3.27.0",
    "gatsby": "^4.6.1",
    "gatsby-plugin-utils": "^3.0.0",
    "jest": "^27.4.7"
  },
  "peerDependencies": {
    "fastify": "^3.19.0",
    "gatsby": "^4.2.0"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
