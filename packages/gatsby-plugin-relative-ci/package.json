{
  "name": "gatsby-plugin-relative-ci",
  "version": "1.0.0",
  "description": "Integrate your Gatsby site with Relative CI",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "relative",
    "ci"
  ],
  "main": "index.js",
  "author": "Alex Moon <alex.jared.moon@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@relative-ci/agent": "^2.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "format": "prettier --write .",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "babel-preset-gatsby-package": "^1.10.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.3.2"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-uc/plugins.git",
    "directory": "packages/gatsby-plugin-relative-ci"
  },
  "bugs": {
    "url": "https://github.com/gatsby-uc/plugins/issues"
  },
  "homepage": "https://github.com/gatsby-uc/plugins/packages/gatsby-plugin-relative-ci#readme"
}
