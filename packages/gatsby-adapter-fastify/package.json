{
  "name": "gatsby-adapter-fastify",
  "description": "Gatsby Adapter for serving gatsby on Node.js using Fastify.",
  "version": "0.14.0",
  "homepage": "https://github.com/gatsby-uc/plugins/blob/main/packages/gatsby-adapter-fastify/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-uc/plugins.git",
    "directory": "packages/gatsby-adapter-fastify"
  },
  "contributors": [
    "Alex Moon <alex.jared.moon@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-adapter",
    "nodejs",
    "server",
    "fastify"
  ],
  "bin": {
    "gserve": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "files": [
    "./dist/*"
  ],
  "scripts": {
    "build": "babel src --out-dir ./dist --ignore \"**/__tests__,**/*.d.ts\" --extensions \".ts,.js\"",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@fastify/accepts": "^4.2.0",
    "@fastify/http-proxy": "^8.4.0",
    "@fastify/middie": "^8.3.0",
    "@fastify/static": "^6.10.2",
    "fastify-plugin": "^4.5.1",
    "fs-extra": "^11.1.1",
    "gatsby-core-utils": "^4.12.0",
    "gatsby-plugin-utils": "^4.12.0",
    "http-status-codes": "^2.2.0",
    "mime": "^3.0.0",
    "open": "^8.4.2",
    "picomatch": "^2.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@types/connect": "^3.4.35",
    "@types/fs-extra": "^9.0.13",
    "@types/mime": "^3.0.1",
    "@types/node": "^18.17.11",
    "@types/picomatch": "^2.3.0",
    "@types/yargs": "^17.0.24",
    "babel-preset-gatsby-package": "^3.11.0",
    "cross-env": "^7.0.3",
    "fastify": "^4.22.0",
    "gatsby": "^5.12.1"
  },
  "peerDependencies": {
    "fastify": "^4.5.3",
    "gatsby": "^5.12.1"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
