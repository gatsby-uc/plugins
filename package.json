{
  "description": "Monorepo for plugins in the Gatsby User Collective",
  "repository": "git@github.com:gatsby-uc/plugins.git",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "ci": "yarn install --frozen-lockfile",
    "format": "yarn run lint -- --write",
    "lint": "prettier **/* --check"
  },
  "devDependencies": {
    "prettier": "^2.3.2"
  }
}
